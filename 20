#include <iostream>
using namespace std;
     
int main()
{
    int a, b, c;
     
    cout << "Masukan Bilangan 1 : ";
    cin >> a;
     
    cout << "Masukan Bilangan 2 : ";
    cin >> b;
     
    c = a % b;
    while(c != 0)
    {
        a = b;
        b = c;
        c = a % b;
    }
     
    cout<< "FPB bilangan 1 dan 2 adlah: " << b << endl;
 
    return 0;
}
